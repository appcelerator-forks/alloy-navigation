"#window" : {
	width: Ti.UI.FILL,
	height: Ti.UI.FILL,
	backgroundColor : "#FFF"
},
"#window[platform=android]" : {
	exitOnClose: true,
	modal: false,
	navBarHidden : true,
	positions : {
		"header" : "top",
		"tabBar" : "top"
	}
},
"#window[platform=ios]" : {
	positions : {
		"header" : "top",
		"tabBar" : "bottom"
	}
}
"#window[platform=mobileweb]" : {
	positions : {
		"header" : "top",
		"tabBar" : "none"
	}
}